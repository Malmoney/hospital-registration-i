  <select id="selectDoctor" parameterType="com.liurq.server.restful.req.hospital.SelectDoctorReq" resultMap="SelectDoctorMap">
    select
        d.*
        ,de.*
        ,hos.*
    from tb_doctor d
        left Join tr_department_doctor ded on ded.doctor_id = d.doctor_id
        LEFT JOIN tb_department de on de.department_id = ded.department_id
        LEFT JOIN tb_hospital hos on hos.hospital_id = ded.hospital_id
    where d.`status` = '0'
        and ded.deleted ='0'
        and de.`status`='0'
        and hos.`status`='0'
    <if test="doctorName != null and doctorName !='' ">
        and d.doctor_name like CONCAT('%',#{doctorName,jdbcType=VARCHAR},'%')
    </if>
    <if test="departmentId != null and departmentId !='' ">
      and de.department_id = #{departmentId,jdbcType=VARCHAR}
    </if>
    <if test="city != null and city !='' ">
      and hos.hospital_city = #{city,jdbcType=VARCHAR}
    </if>
  </select>
